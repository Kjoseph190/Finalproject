function openInstagram() {
    const instagramUsername = 'kjvision_';
    const instagramUrl = `https://www.instagram.com/kjvision_?igsh=YzZjenhubGMybjB3&utm_source=qr${kjvision_}/`;
  
    window.open(instagramUrl, '_blank');
  }
  function zoomImage(thumbnail) {
    var zoomedImageContainer = document.querySelector('.zoomed-image-container');
    var zoomedImage = document.querySelector('.zoomed-image');

    zoomedImage.src = thumbnail.src;
    zoomedImageContainer.style.display = 'flex';

    zoomedImageContainer.onclick = function() {
        zoomedImageContainer.style.display = 'none';
    };
}